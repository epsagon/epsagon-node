{
  "name": "epsagon",
  "version": "0.0.0-development",
  "description": "Epsagon Instrumentation for Node.js",
  "keywords": [
    "serverless",
    "epsagon",
    "tracing",
    "distributed-tracing",
    "lambda",
    "aws-lambda",
    "debugging",
    "monitoring"
  ],
  "author": "Epsagon Team <support@epsagon.com>",
  "license": "MIT",
  "scripts": {
    "pretest": "./scripts/build_peg.sh",
    "test": "./scripts/run_tests.sh",
    "test-with-coverage": "nyc --reporter=text npm run test",
    "lint:js": "eslint --max-warnings=0 ./src/ ./test/unit_tests -f table --ext .js --ext .jsx",
    "lint:js:fix": "eslint --max-warnings=0 ./src/ ./test/unit_tests -f table --ext .js --ext .jsx --fix",
    "lint": "npm run lint:js",
    "build:dev": "./scripts/build_peg.sh && rollup -c",
    "build": "./scripts/build_peg.sh && NODE_ENV=production rollup -c",
    "clean": "rm -r dist/",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release"
  },
  "bugs": {
    "url": "https://github.com/epsagon/epsagon-node/issues"
  },
  "homepage": "https://github.com/epsagon/epsagon-node#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/epsagon/epsagon-node.git"
  },
  "main": "dist/bundle.js",
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/runtime": "^7.12.13",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "aws-sdk": "^2.843.0",
    "body-parser": "^1.19.0",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "husky": "^5.0.9",
    "ldap-server-mock": "^3.0.0",
    "ldapjs": "^2.2.3",
    "lolex": "^5.1.2",
    "mocha": "^8.3.0",
    "mongodb": "^3.6.4",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "nyc": "^15.1.0",
    "pegjs": "^0.10.0",
    "pg": "^8.5.1",
    "pg-pool": "^3.2.2",
    "proxyquire": "^2.1.3",
    "randomstring": "^1.1.5",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.7",
    "rollup": "^2.39.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.3.9",
    "semver": "^7.3.4",
    "simple-oauth2": "^4.2.0",
    "sinon": "^9.2.4",
    "uglify-es": "^3.3.9",
    "underscore": "^1.12.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "google-protobuf": "^3.14.0",
    "json.sortify": "^2.2.2",
    "md5": "^2.3.0",
    "require-in-the-middle": "^5.1.0",
    "shimmer": "^1.2.1",
    "uuid-parse": "^1.1.0",
    "uuid4": "^2.0.2"
  }
}
